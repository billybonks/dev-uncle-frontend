{{log @onPropertyChange}}
{{#let (object-keys @propertyDetails) as |properties|}}
  {{#component @comparatorLayout property=this.property as |layout|}}
    {{#if layout.property}}
      <PowerSelectBlockless
        @options={{properties}}
        @selected={{this.property}}
        @onchange={{@onPropertyChange}}
        class="ember-basic-dropdown"
      />
    {{/if}}
    {{#if layout.comparator}}
      <PowerSelectBlockless
        @options={{this.comparatorOptions}}
        @selected={{this.selectedComparator}}
        @labelPath="key"
        @onchange={{@onComparatorChange}}
        class="ember-basic-dropdown"
      />
    {{/if}}
    {{#if layout.editor}}
      {{yield this.selectedComparator}}
    {{/if}}
  {{/component}}
{{/let}}
